    "module": "./dist/index.js",
    "import": "./dist/index.js",
    "default": "./dist/index.js"
  },
  "devDependencies": {
    "@babel/types": "^7.24.0",
    "@types/node": "^20.12.2",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3",
    "vite": "^5.2.7"
